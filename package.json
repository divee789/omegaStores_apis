{
  "name": "jiji-clone-apis",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/server.js",
    "start:dev": "ts-node-dev --respawn ./src/server.ts",
    "transpile": "babel ./src --out-dir dist",
    "build": "tsc",
    "clean": "rimraf dist/*",
    "seed": "sequelize db:seed",
    "seed:all": "sequelize db:seed:all --debug",
    "seed:undo": "sequelize db:seed:undo",
    "seed:undo:all": "sequelize db:seed:undo:all",
    "migrate": "sequelize db:migrate",
    "migrate:all": "sequelize db:migrate:all",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:undo:all": "sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@sendgrid/mail": "^7.2.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.7",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/hapi__joi": "^17.1.4",
    "@types/helmet": "0.0.48",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.6.3",
    "@types/validator": "^13.1.0",
    "axios": "^0.20.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.23.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "datauri": "^2.0.0",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "google-auth-library": "^6.0.6",
    "helmet": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "sequelize": "^6.3.5",
    "uuid": "^8.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.0.0-pre.61",
    "typescript": "^4.0.2"
  }
}
